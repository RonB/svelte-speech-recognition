{
	"name": "svelte-speech-recognition",
	"version": "0.0.7",
	"author": "Josh Hubbard <josh@joshuahubbard.dev> (https://joshuahubbard.dev/)",
	"description": "A Svelte wrapper for the Sortable.js library",
	"repository": {
		"type": "git",
		"url": "git://github.com/jhubbardsf/svelte-speech-recognition.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "vite package",
		"preview": "vite preview",
		"prepare": "vite sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"debugger": "NODE_OPTIONS=\"--inspect\" vite dev"
	},
	"devDependencies": {
		"@speechly/speech-recognition-polyfill": "^1.1.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.4.1",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"autoprefixer": "^10.4.4",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.12",
		"postcss-load-config": "^3.1.4",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"svelte": "^3.46.0",
		"svelte-check": "^2.2.6",
		"vite": "^1.0.0",
		"svelte-preprocess": "^4.10.5",
		"svelte2tsx": "^0.5.10",
		"tailwindcss": "^3.0.23",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2"
	},
	"type": "module",
	"keywords": [
		"svelte",
		"vite",
		"speech-recognition"
	],
	"license": "MIT",
	"dependencies": {
		"@sveltejs/vite-plugin-svelte": "^1.2.0",
		"vite": "^3.2.4"
	}
}